<ResourceDictionary xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Contains resource overrides for third-party library themes  -->

    <!--  FluentAvalonia overrides  -->
    <Thickness x:Key="ContentDialogPadding">24,12,24,12</Thickness>
    <x:Double x:Key="ContentDialogMinHeight">160</x:Double>

    <!--  Dock overrides  -->
    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="{DynamicResource ThemeBackgroundColor}" />

    <!-- <StaticResource x:Key="TextBoxDefaultBackground" ResourceKey="SemiColorFill0" /> -->
    <SolidColorBrush x:Key="TextBoxDefaultBackground" Opacity="0.12" Color="{StaticResource SemiWhiteColor}" />

    <SolidColorBrush x:Key="TextBoxPointeroverBackground" Opacity="0.16" Color="{StaticResource SemiWhiteColor}" />
    <SolidColorBrush x:Key="TextBoxPressedBackground" Opacity="0.20" Color="{StaticResource SemiWhiteColor}" />
    <!-- <StaticResource x:Key="TextBoxDisabledBackground" ResourceKey="SemiColorDisabledFill" /> -->
</ResourceDictionary>
